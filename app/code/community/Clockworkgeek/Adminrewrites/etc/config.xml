<config>
	<modules>
		<Clockworkgeek_Adminrewrites>
			<version>1.0.0</version>
		</Clockworkgeek_Adminrewrites>
	</modules>
	<global>
		<models>
			<adminrewrites>
				<class>Clockworkgeek_Adminrewrites_Model</class>
			</adminrewrites>
			<core>
				<rewrite>
					<store>Clockworkgeek_Adminrewrites_Model_Store</store>
				</rewrite>
			</core>
		</models>
	</global>
	<adminhtml>
		<translate>
			<modules>
				<Clockworkgeek_Adminrewrites>
					<files>
						<default>Clockworkgeek_Adminrewrites.csv</default>
					</files>
				</Clockworkgeek_Adminrewrites>
			</modules>
		</translate>
	</adminhtml>
</config>
